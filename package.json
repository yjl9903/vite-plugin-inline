{
  "name": "vite-plugin-inline",
  "version": "0.0.3",
  "description": "Inline raw TypeScript code as a string",
  "keywords": [
    "inline",
    "static-assets",
    "typescript",
    "vite",
    "vite-plugin"
  ],
  "homepage": "https://github.com/yjl9903/vite-plugin-inline#readme",
  "bugs": {
    "url": "https://github.com/yjl9903/vite-plugin-inline/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjl9903/vite-plugin-inline.git"
  },
  "license": "MIT",
  "author": "XLor",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm -C examples/vite dev",
    "format": "prettier --write src/**/*.ts",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "preversion": "pnpm typecheck && pnpm build",
    "postversion": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  },
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "packageManager": "pnpm@10.2.0"
}